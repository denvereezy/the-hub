{{#donor}}
<div>
  <!-- Nav tabs -->
  <ul class="nav nav-tabs" role="tablist">
    <li role="presentation" class="active"><a href="#home" aria-controls="home" role="tab" data-toggle="tab">Questionnaires</a></li>
    <li role="presentation"><a href="#profile" aria-controls="profile" role="tab" data-toggle="tab">Setup Questionnaire</a></li>
  </ul>
  <!-- Tab panes -->
  <div class="tab-content">
    <div role="tabpanel" class="tab-pane active" id="home">
      <table class="table table-striped">
        <thead class="thead-inverse">
          <tr>
            <th>Questionnaire name:</th>
            <th>Due Date:</th>
            <th>Action:</th>
          </tr>
        </thead>
        {{#each donorQuestionnaires}}
        <tr>
          <td>{{name}}</td>
          <td>{{dueDate}}</td>
          <td><a href="/questionnaire/{{id}}/questions/{{base_questionnaire_id}}/results"><button class="btn btn-primary">View</button></a></td>
        </tr>
        {{/each}}
      </table>
      <!-- <button class="btn btn-success" onclick="goBack()">back</button> -->
    </div>
<div role="tabpanel" class="tab-pane" id="profile">
    <form class="col-md-8" action="/questionnaire/setup/step1" method="POST">
      <h2>Setup Questionnaire</h2>
      <div class="form-group">
        <label>Questionnaire Name:</label><input type="text" class="form-control" placeholder="ex. mLab" name="name" />
        <label>Due date:</label><input type="date" class="form-control" name="dueDate" />
      </div>
      <!-- <button class="btn btn-success" onclick="goBack()">back</button> -->
      <input type="submit" class="btn btn-primary pull-right">
    </form>
  </div>
  </div>
{{/donor}}
{{#facilitator}}
<ul class="nav nav-pills col-md-8">
  <li class="active">
    <a href="/view-questionnaire">View Questionnaires</a>
  </li>
  <li>
    <a href="/view-questionnaires">Questionnaire Results</a>
  </li>
</ul>
{{/facilitator}}

{{#startup}}
<ul class="nav nav-pills col-md-8">
  <li class="active">
    <a href="/answer-questionnaire">Answer Questionnaires</a>
  </li>
</ul>
{{/startup}}
